<header class="top-page-header2">
  <div class="title">
    <%= link_to "Cattle_Feed", cows_path, class: "title-text"%>
  </div>
  <div class="sub-contents">
    <div class="back">
      <%= link_to "戻る", feeds_path, class: 'btn btn-default back-btn'%>
    </div>
  </div>
</header>
<div class="feed-show-main">
  <div class="feed-show-btn">
    <div class="f-edit">
      <%= link_to "編集", edit_feed_path(@feed.id), class:"btn btn-default feed-edit" %>
    </div>
    <div class="f-destroy">
      <%= link_to "削除", feed_path(@feed.id),method: :delete, class:"btn btn-default feed-destroy" %>
    </div>
  </div>
  <div class="feed-name">
    <span class="feed-name-title">名称：</span><%= @feed.name %>
  </div>
  <div class="feed-price">
    <span class="feed-price-title">価格：</span><%= @feed.unit_price %><span class="feed-price-unit">円/kg</span>
  </div>
  <div class="feed-nutrients">
    <div class="feed-nutrients-title">
      栄養価（1kg当たり）
    </div>
    <div class="feed-nutrients-lists">
      タンパク質 10g
    </div>
  </div>
</div>