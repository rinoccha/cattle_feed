<header class="top-page-header2">
  <div class="title">
    <%= link_to "Cattle_Feed", cows_path, class: "title-text"%>
  </div>
  <div class="sub-contents">
    <div class="feed-plus">
      <%= link_to "追加", new_feed_path, class: 'btn' %>
    </div>
    <div class="back-btn">
      <%= link_to "戻る", cows_path, class: 'btn'%>
    </div>
  </div>
</header>
<div class="feed-main">
  <div class="feed-title">
    餌一覧
  </div>
  <div class="feed-lists">
    <% @feeds.each do |feed|%>
      ・<%= link_to "#{feed.name}", feed_path(feed.id), class:"btn" %>
    <% end %>
  </div>
</div>